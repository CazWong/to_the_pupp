<div class="container">

<h2>Add a new puppy</h2>

<%= simple_form_for @puppy do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :breed %>
  <%= f.input :description %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit, "Submit!", class:"btn btn-primary" %>



<% end %>



<%= link_to puppies_path do %>

<% end %>
</div>
